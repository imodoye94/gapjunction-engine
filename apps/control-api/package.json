{
  "name": "@gapjunction/control-api",
  "version": "0.1.0",
  "description": "Gap Junction Control API - Orchestrates compile, deploy, and monitor workflows",
  "private": true,
  "license": "MPL-2.0",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "tsx watch src/main.ts",
    "start:debug": "ts-node-dev --debug --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:e2e": "vitest run --config ./test/vitest-e2e.config.ts",
    "test:integration": "vitest run test/control-api.integration.test.ts test/websocket.integration.test.ts test/supabase.integration.test.ts",
    "test:performance": "vitest run test/performance.test.ts --reporter=verbose",
    "test:security": "vitest run test/security.test.ts --reporter=verbose",
    "test:api": "vitest run test/api.e2e-spec.ts",
    "test:unit": "vitest run src/**/*.spec.ts",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:api && pnpm run test:performance && pnpm run test:security",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@gapjunction/ir-schema": "workspace:*",
    "@supabase/supabase-js": "^2.38.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "msgpack-lite": "^0.1.26",
    "socket.io": "^4.7.0",
    "ulid": "^2.3.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/msgpack-lite": "^0.1.8",
    "@types/node": "^20.5.2",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "tsx": "^4.20.5",
    "typescript": "^5.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "fastify",
    "control-api",
    "orchestration",
    "websocket",
    "healthcare",
    "integration",
    "gapjunction"
  ]
}